{
    "name": "xiaohuilam/laravel-test",
    "description": "laravel abstract testcase class adapter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@aliyun.com"
        }
